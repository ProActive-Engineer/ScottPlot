<Window x:Class="ScottPlotQuickstartWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ScottPlotQuickstartWPF"
        mc:Ignorable="d"
        Title="ScottPlot - WPF Demo" Height="318" Width="537.895" Loaded="Window_Loaded">
    <Grid>
        <Canvas Name="canvasPlot" Height="Auto" Width="Auto">
            <Image Name="imagePlot" Width="{Binding ActualWidth}" Height="{Binding ActualHeight}"/>
        </Canvas>
    </Grid>
</Window>
